{
  "name": "fabric-stock",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"cross-env ELECTRON_START_URL=http://localhost:3000 next dev --turbopack\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "cross-env NEXT_DISABLE_ESLINT=1 next build && next export -o out",
    "start": "electron .",
    "lint": "next lint",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.fabricstock.app",
    "files": [
      "electron/",
      "out/",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^7.3.2",
    "@prisma/client": "^6.16.2",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.539.0",
    "next": "15.4.5",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.1",
    "serialport": "^13.0.0",
    "swr": "^2.3.6",
    "ts-node": "^10.9.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/file-saver": "^2.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^8.0.4"
  }
}
